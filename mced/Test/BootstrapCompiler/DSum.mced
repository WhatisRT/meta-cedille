module DSum.

elet DSum (A : *) (B : A -> *) : * := ∀ X : *. (Π a : A. B a -> X) -> X.

elet pair [A : *] [B : A -> *] (a : A) (b : B a) : DSum A B :=
  Λ X : *. λ p : Π a : A. B a -> X. p a b.

elet recursionDSum [A : *] [B : A -> *] [X : *] (ppair : Π a : A. B a -> X) (x : DSum A B)
  : X := x ?X ppair.

endModule.
