-- This is a simple example that defines an alternate projection syntax.

let init$proj := ∀ X : * (Π _ : (Π _ : init$term X) (Π _ : (Π _ : init$term X) X)) : *.

b-let init$term$=alpha=_proj_ (p : init$proj) : init$term :=
  [p ?init$term λ t : init$term [quote$pi t] λ t : init$term [quote$psi t]].

b-let init$proj$1_space__term_ (_ : init$space) (x : init$term) : init$proj :=
  Λ X : * λ t : (Π _ : init$term X) λ t' : (Π _ : init$term X) [t x].

b-let init$proj$2_space__term_ (_ : init$space) (x : init$term) : init$proj :=
  Λ X : * λ t : (Π _ : init$term X) λ t' : (Π _ : init$term X) [t' x].

seteval eval init stmt.