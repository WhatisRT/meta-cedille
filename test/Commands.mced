b-let falseCommand : init$name := γ[quote$stringToName quote$a ""].

b-let init$stmt'$=sigma=cmd_space_=doublequote=_newstring_=doublequote=_space'_=dot=
  (_ : init$space) (s : init$string) (_ : init$space') : init$stmt' :=
  [quote$cmd γ[stringToNameDefault s falseCommand]].

b-let init$stmt'$echo_space_=doublequote=_newstring_=doublequote=_space'_=dot=
  (_ : init$space) (s : init$string) (_ : init$space') : init$stmt' :=
  [quote$cmd γ[stringToNameDefault γ[stringApp "echo " s] falseCommand]].

seteval eval init stmt.