b-let Maybe (A : *) : * := [Sum A Unit].

b-let maybe [A : *] [B : *] (default : B) (f : Π _ : A B) (x : [Maybe A]) : B :=
  [inductionSum ?A ?Unit ?(λ _ : [Maybe A] B) (λ a : A [f a]) (λ _ : Unit default) x].

b-let maybeToList [A : *] (x : [Maybe A]) : [List A] :=
  [maybe ?A ?[List A] τ[A|] (λ a : A τ[A|a]) x].